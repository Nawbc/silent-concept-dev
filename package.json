{
	"author": "Han Wang",
	"version": "0.0.1",
	"name": "silent-concept",
	"description": "",
	"main": "./release/lib/index.js",
	"module": "./release/es/index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=development IS_ZIP=false node bin/dev.server.js",
		"release:dist": "rimraf release/dist && rimraf release/components && npm-run-all dist:*",
		"dist:zip": "cross-env NODE_ENV=production IS_ZIP=true node ./bin/release.dist.js",
		"dist:unzip": "cross-env NODE_ENV=production IS_ZIP=false node ./bin/release.dist.js",
		"release:app": "rimraf release/app && cross-env NODE_ENV=production IS_ZIP=true node ./bin/release.app.js",
		"es": "node ./bin/release.lib.js es",
		"es:tsc": "tsc --outDir ./release/es -p ./includeLib.json",
		"es:dts": "tsc -d --emitDeclarationOnly -p ./includeLib.json --declarationDir ./release/es",
		"lib": "node ./bin/release.lib.js  lib",
		"lib:babel": "cross-env NODE_ENV=production IS_RENAME=true babel lib --out-dir release/lib --extensions .tsx --source-maps",
		"lib:dts": "tsc -d --emitDeclarationOnly -p ./includeLib.json --declarationDir ./release/lib",
		"release": "npm run dist && npm run app && npm run lib && npm run es && npm run plugins",
		"scss:lib": "node-sass lib -o release/lib --importer ./scripts/utils/compass.js --source-map true --output-style compressed",
		"scss:es": "node-sass lib -o release/es --importer ./scripts/utils/compass.js --source-map true --output-style compressed",
		"test": "npm run test:unit && npm run test:e2e",
		"test:unit": "jest -u",
		"test:e2e": "jest --config ./jest.e2e.js",
		"test:ci": "cross-env CI=true node ./scripts/utils/ciTest.js",
		"auto": "npm-run-all auto:*",
		"auto:lang": "node ./auto/langTypesMaker.js",
		"auto:color": "node ./auto/gradientsMaker.js",
		"auto:svg": "node ./auto/`svgModuleMaker`.js",
		"eslint": "eslint ./ --ext .ts,.tsx,.js,.jsx --fix",
		"eslint:test": "eslint ./lib/index.tsx --fix",
		"cz:init": "commitizen init cz-conventional-changelog --save --save-exact",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
	},
	"engines": {
		"npm": "6.10.1",
		"node": ">=10.13.0"
	},
	"config": {
		"port": 3000,
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		},
		"webgradients": "https://webgradients.com/",
		"libSvg": {
			"target": "./lib/assets/svg",
			"src": "./auto/svg"
		}
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"component-style": "0.0.1",
		"computed-style": "^0.3.0",
		"core-js": "^3.2.1",
		"height-zero2auto": "^1.1.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-spring": "^8.0.27",
		"resize-observer-polyfill": "^1.5.1"
	},
	"peerDependencies": {
		"prop-types": "^15.6.0",
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "7.4.3",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "^7.6.3",
		"@babel/preset-react": "^7.6.3",
		"@babel/preset-typescript": "^7.3.3",
		"@types/classnames": "^2.2.9",
		"@types/jest": "^24.0.16",
		"@types/node": "^12.6.8",
		"@types/react": "^16.8.23",
		"@types/react-dom": "^16.8.5",
		"@types/react-intl": "^2.3.18",
		"@types/react-router-dom": "^4.3.4",
		"@typescript-eslint/eslint-plugin": "^2.3.3",
		"@typescript-eslint/parser": "^2.3.3",
		"axios": "^0.19.0",
		"babel-jest": "^24.8.0",
		"babel-loader": "8.0.5",
		"babel-plugin-import": "^1.12.0",
		"babel-plugin-transform-rename-import": "^2.3.0",
		"cheerio": "^1.0.0-rc.3",
		"commitizen": "^4.0.3",
		"compass-importer": "^0.4.1",
		"conventional-changelog-cli": "^2.0.23",
		"cross-env": "^5.2.0",
		"css-loader": "2.1.1",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.14.0",
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-loader": "^3.0.2",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.14.3",
		"eslint-plugin-react-hooks": "^2.1.2",
		"file-loader": "^3.0.1",
		"fork-ts-checker-webpack-plugin": "^1.5.0",
		"fs-extra": "^8.1.0",
		"html-webpack-plugin": "^4.0.0-beta.8",
		"husky": "^3.0.9",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^24.7.1",
		"jest-puppeteer": "^4.3.0",
		"lint-staged": "^9.2.5",
		"mini-css-extract-plugin": "0.5.0",
		"node-sass": "^4.12.0",
		"npm-run-all": "^4.1.5",
		"opn": "^6.0.0",
		"optimize-css-assets-webpack-plugin": "5.0.1",
		"prettier": "^1.18.2",
		"puppeteer": "^1.20.0",
		"react-flexbox-grid": "^2.1.2",
		"react-intl": "^2.9.0",
		"react-router-dom": "^5.0.1",
		"react-syntax-highlighter": "^11.0.2",
		"react-test-renderer": "^16.8.6",
		"rimraf": "^3.0.0",
		"sass-loader": "7.1.0",
		"style-loader": "0.23.1",
		"svg-to-react": "^0.1.0",
		"svgo": "^1.3.0",
		"terser-webpack-plugin": "1.2.3",
		"thread-loader": "^2.1.3",
		"typescript": "^3.6.3",
		"url-loader": "1.1.2",
		"webpack": "4.35.0",
		"webpack-dev-server": "3.2.1",
		"webpack-hot-dev-clients": "^2.0.2",
		"webpack-manifest-plugin": "2.0.4",
		"workbox-webpack-plugin": "4.2.0"
	},
	"license": "Apache-2.0",
	"repository": "https://github.com/sewerganger/silent-concept"
}
